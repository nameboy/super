@import "./styles/iconfont.wxss";
/* 在小程序中是不支持通配符的 */
page,view,text,switch,swiper-item,image,navigator{
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}
/* 主题颜色  通过变量实现
   
*/
page{
    /* 定义主题颜色 */
    --themeColor:#eb4450; 
    /* 
  定义统一的字体大小，大小是  375px
  1px==2rpx
  */
  font-size: 28rpx;
}
image{
  width: 100%;
}